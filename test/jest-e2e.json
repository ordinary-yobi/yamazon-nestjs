{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@lib/user-location/(.*)": "<rootDir>/../libs/user-location/src/$1",
    "@lib/user-location": "<rootDir>/../libs/user-location/src"
  }
}